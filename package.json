{
  "name": "type-realm",
  "version": "0.0.1",
  "description": "A persistent, typed-objects implementation.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "bluebird": "^3.3.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.1",
    "type-registry": "0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.6.4",
    "babel-plugin-contracts": "^1.1.1",
    "babel-plugin-trace": "^1.0.0",
    "babel-plugin-transform-async-to-module-method": "^6.5.2",
    "babel-plugin-typecheck": "^3.6.1",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node5": "^1.1.2",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "flow-bin": "^0.22.0",
    "isparta": "^4.0.0",
    "mocha": "^2.3.3",
    "should": "^7.1.1"
  },
  "scripts": {
    "prepublish": "NODE_ENV=production npm run build && npm test",
    "test": "flow check && mocha --max-old-space-size=8192 --expose-gc",
    "watch": "mocha --watch --max-old-space-size=8192 --expose-gc",
    "build": "babel -d ./lib ./src",
    "coverage": "NODE_ENV=coverage babel-node node_modules/isparta/bin/isparta cover --report text --report html node_modules/mocha/bin/_mocha -- --reporter dot --bail",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codemix/type-realm.git"
  },
  "keywords": [
    "type",
    "types",
    "type system",
    "type registry",
    "static typing"
  ],
  "author": "Charles Pick <charles@codemix.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codemix/type-realm/issues"
  },
  "homepage": "https://github.com/codemix/type-realm#readme"
}
